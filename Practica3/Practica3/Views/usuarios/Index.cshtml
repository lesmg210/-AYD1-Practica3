@model IEnumerable<Practica3.Datos.usuario>

@{
    ViewBag.Title = "Catálogo de Usuarios";
}

<h2>Catálogo de Clientes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreCompleto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CorreoElectronico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contraseña)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreCompleto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CorreoElectronico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contraseña)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CodUsuario }) |
            @Html.ActionLink("Details", "Details", new { id=item.CodUsuario }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CodUsuario })
        </td>
    </tr>
}

</table>
